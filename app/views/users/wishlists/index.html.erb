<div class="row">
  <div class="col-3">
    <%= render "shared/user/sidebar" %>
  </div>
  <div class="col-9">
    <%= turbo_frame_tag "tables" do %>
      <div class="row">
        <% if @wishlists.present? %>
          <%= render partial: "wishlist", collection: @wishlists %>
          <%= render "shared/pagy", pagy: @pagy %>
        <% else %>
          <div class="text-center">
            <h1>Data Not Found</h1>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
</div>

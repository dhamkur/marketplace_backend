<%= turbo_frame_tag "tables" do %>
  <div class="table-responsive mb-5">
    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>Created At</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody id="lists">
        <%= render @objects %>
      </tbody>
    </table>
  </div>

  <%= render "shared/pagy", pagy: @pagy %>
<% end %>
